{
  "name": "react-transition-state",
  "version": "1.1.0",
  "description": "Perform animation and transition of React component with ease.",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": "szhsin/react-transition-state",
  "homepage": "https://szhsin.github.io/react-transition-state/",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "transition",
    "animation",
    "component",
    "hook"
  ],
  "scripts": {
    "start": "rollup -c -w",
    "clean": "rm -Rf dist",
    "bundle": "rollup -c",
    "test": "jest",
    "eg": "npm start --prefix example",
    "dtslint": "dtslint --localTs node_modules/typescript/lib types",
