    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pret": "prettier --write .",
    "build": "run-s clean lint pret bundle types"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.1",
    "dtslint": "^4.1.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.56.3",
    "typescript": "^4.4.2"
  }
